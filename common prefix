#include <stdio.h>

int main()
{
    char s1[50],s2[50],k[15];
    int i,j;
    scanf("%s",&s1);
    scanf("%s",&s2);
    for(i=0,j=0;s1[i]!='\0'&& s2[j]!='\0';i++,j++)
    {
        if(s1[i]==s2[j])
        {
            k[i]=s1[i];
            
        }
        else
        {
            k[i]='\0';
        }
    }
    printf("%s",k);

    return 0;
}

